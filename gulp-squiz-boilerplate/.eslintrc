{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "block-scoped-var": 2,
    "complexity": [1, 5],
    "camelcase": 2,
    "comma-style": [ 1, "last" ],
    "curly": [ 2, "all" ],
    "dot-notation": [ 2, { "allowKeywords": false, "allowPattern": "^catch" } ],
    "eqeqeq": [ 2, "allow-null" ],
    "global-strict": [ 1, "always" ],
    "guard-for-in": 2,
    "key-spacing": [ 2, { "align": "value", "beforeColon": false, "afterColon": true } ],
    "new-cap": 2,
    "no-bitwise": 2,
    "no-caller": 2,
    "no-cond-assign": 2,
    "no-debugger": 2,
    "no-empty": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-parens": 2,
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-multi-spaces": 2,
    "no-new": 2,
    "no-plusplus": 2,
    "no-proto": 2,
    "no-script-url": 2,
    "no-sequences": 2,
    "no-shadow": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,
    "no-with": 2,
    "quotes": [ 2, "single" ],
    "semi": [ 2, "always" ],
    "space-infix-ops": 0,
    "valid-typeof": 2,
    "wrap-iife": [ 2, "outside" ]
  }
}